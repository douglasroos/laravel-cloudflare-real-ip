    "keywords": [
        "laravel",
        "cloudflare",
        "real ip address"
    ],
    "homepage": "https://github.com/molayli/laravel-cloudflare-real-ip",
    "authors": [
        {
            "name": "Billal",
            "email": "hello@molayli.com"
        }
    ],
    "require": {
        "php" : ">=7.0"
    },
    "autoload" : {
        "psr-4" : {
            "Molayli\\" : "src"
        }
    },
    "extra": {
        "laravel": {
